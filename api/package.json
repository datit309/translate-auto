    "moment": "^2.29.4",
    "moment-timezone": "^0.5.41",
    "mongoose": "^6.6.2",
    "mongoose-autopopulate": "^0.16.1",
    "mongoose-paginate-v2": "^1.7.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "node-telegram-bot-api": "^0.61.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "puppeteer": "^21.0.3",
    "random-useragent": "^0.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.23.1",
    "sequelize-typescript": "^2.1.3",
    "slugify": "^1.6.5",
    "uuid": "^9.0.0"
