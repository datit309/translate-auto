{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "deploy": "sh install-product.bash",
    "deploy-dev": "sh install-testnet.bash",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "build-dev": "cross-env NODE_ENV=development nest build && cp .env.development ./dist/.env",
    "build-testnet": "cross-env NODE_ENV=production nest build && cp .env.testnet ./dist/.env",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.0.1",
    "@google-cloud/translate": "^8.0.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "9.1.6",
    "@nestjs/platform-ws": "^9.4.2",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/websockets": "9.1.6",
    "@types/cache-manager": "^4.0.2",
    "@vitalets/google-translate-api": "^9.2.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cache-manager": "^4.1.0",
    "cache-manager-mongoose": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-mongodb-session": "^3.1.1",
    "dotenv": "^16.0.3",
    "ethers": "^6.3.0",
    "express-session": "^1.17.3",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
